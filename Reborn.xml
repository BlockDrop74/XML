   <team color="blue" max=“15” max-overfill=“25”>Blue Team</team>
</teams>
<rules>
    <rule>Don’t hoard team supplies!</rule>
</rules>
<kits>
    <kit name="spawn">
        <item slot="0">stone sword</item>
        <item slot=“1”>bow</item>
        <item slot="2" enchantment="durability:3">iron pickaxe</item>
        <item slot=“3”>iron axe</item>
        <item slot=“4” amount=“32” damage="1">log</item>
        <item slot=“5” amount="64">glass</item>
        <item slot=“6” amount="64">golden carrot</item>
        <item slot=“7” amount="1">golden apple</item>
        <item slot=“28” amount="64">arrow</item>
        <potion duration="5" amplifier="1">heal</potion>
    </kit>
    <kit name="pink" parents="spawn">
        <helmet color="ff69b4">leather helmet</helmet>
        <chestplate color="ff69b4">leather chestplate</chestplate>
        <leggings color="ff69b4">leather leggings</leggings>
        <boots color="ff69b4">leather boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="0066cc">leather helmet</helmet>
        <chestplate color="0066cc">leather chestplate</chestplate>
        <leggings color="0066cc">leather leggings</leggings>
        <boots color="0066cc">leather boots</boots>
    </kit>
</kits>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<itemremove>
    <item>golden carrot</item>
    <item>golden apple</item>
    <item>diamond helmet</item>
    <item>leather chestplate</item>
    <item>leather helmet</item>
    <item>leather leggigns</item>
    <item>leather boots</item>
    <item>glass</item>
    <item>clay</item>
    <item>string</item>
</itemremove>
<timelock>on</timelock>
<maxbuildheight>95</maxbuildheight>
<difficulty>1</difficulty>
<hunger>
    <depletion>off</depletion>
</hunger>
