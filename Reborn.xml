<?xml version="1.0"?>
<map proto="1.3.0">
<name>Reborn</name>
<version>1.1</version>
<objective>Capture the enemy team’s two wools and return them to your wool monument!</objective>
<authors>
    <author>feidi_22</author>
</authors>
<contributors>
    <contributor contribution=“XML help”>triplezko</contributor>
    <contributor contribution=“Server host”>elego123</contributor>
</contributors>
<teams>
